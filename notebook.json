{
    "apiVersion":"kubeflow.org/v1",
    "kind":"Notebook",
    "metadata":{
       "labels":{
          "app":"my-new-workbench",
          "opendatahub.io/odh-managed":"true",
          "opendatahub.io/user":"user2",
          "opendatahub.io/dashboard":"true"
       },
       "annotations":{
          "openshift.io/display-name":"my-new-workbench",
          "openshift.io/description":"",
          "notebooks.opendatahub.io/oauth-logout-url":"https://rhods-dashboard-redhat-ods-applications.apps.cluster-kfg6p.kfg6p.sandbox2010.opentlc.com/projects/deteccao-de-fraude?notebookLogout=my-new-workbench",
          "notebooks.opendatahub.io/last-size-selection":"Standard",
          "notebooks.opendatahub.io/last-image-selection":"s2i-minimal-notebook:2024.1",
          "notebooks.opendatahub.io/inject-oauth":"true",
          "opendatahub.io/username":"user2",
          "opendatahub.io/accelerator-name":"",
          "opendatahub.io/image-display-name":"Minimal Python"
       },
       "name":"my-new-workbench",
       "namespace":"deteccao-de-fraude"
    },
    "spec":{
       "template":{
          "spec":{
             "affinity":{
                
             },
             "enableServiceLinks":false,
             "containers":[
                {
                   "image":"image-registry.openshift-image-registry.svc:5000/redhat-ods-applications/s2i-minimal-notebook:2024.1",
                   "imagePullPolicy":"Always",
                   "workingDir":"/opt/app-root/src",
                   "name":"my-new-workbench",
                   "env":[
                      {
                         "name":"NOTEBOOK_ARGS",
                         "value":"--ServerApp.port=8888\n                  --ServerApp.token=\\'\\'\n                  --ServerApp.password=\\'\\'\n                  --ServerApp.base_url=/notebook/deteccao-de-fraude/my-new-workbench\n                  --ServerApp.quit_button=False\n                  --ServerApp.tornado_settings={\"user\":\"user2\",\"hub_host\":\"https://rhods-dashboard-redhat-ods-applications.apps.cluster-kfg6p.kfg6p.sandbox2010.opentlc.com\",\"hub_prefix\":\"/projects/deteccao-de-fraude\"}"
                      },
                      {
                         "name":"JUPYTER_IMAGE",
                         "value":"image-registry.openshift-image-registry.svc:5000/redhat-ods-applications/s2i-minimal-notebook:2024.1"
                      }
                   ],
                   "envFrom":[
                      
                   ],
                   "resources":{
                      "limits":{
                         "cpu":"2",
                         "memory":"8Gi"
                      },
                      "requests":{
                         "cpu":"1",
                         "memory":"6Gi"
                      }
                   },
                   "volumeMounts":[
                      {
                         "mountPath":"/opt/app-root/src",
                         "name":"my-new-workbench"
                      },
                      {
                         "name":"shm",
                         "mountPath":"/dev/shm"
                      }
                   ],
                   "ports":[
                      {
                         "name":"notebook-port",
                         "containerPort":8888,
                         "protocol":"TCP"
                      }
                   ],
                   "livenessProbe":{
                      "initialDelaySeconds":10,
                      "periodSeconds":5,
                      "timeoutSeconds":1,
                      "successThreshold":1,
                      "failureThreshold":3,
                      "httpGet":{
                         "scheme":"HTTP",
                         "path":"/notebook/deteccao-de-fraude/my-new-workbench/api",
                         "port":"notebook-port"
                      }
                   },
                   "readinessProbe":{
                      "initialDelaySeconds":10,
                      "periodSeconds":5,
                      "timeoutSeconds":1,
                      "successThreshold":1,
                      "failureThreshold":3,
                      "httpGet":{
                         "scheme":"HTTP",
                         "path":"/notebook/deteccao-de-fraude/my-new-workbench/api",
                         "port":"notebook-port"
                      }
                   }
                }
             ],
             "volumes":[
                {
                   "name":"my-new-workbench",
                   "persistentVolumeClaim":{
                      "claimName":"my-new-workbench"
                   }
                },
                {
                   "name":"shm",
                   "emptyDir":{
                      "medium":"Memory"
                   }
                }
             ],
             "tolerations":[
                
             ]
          }
       }
    }
 }
